{
	"dataset": {
		"name": "Ninapro-DB1",
		"train_generator": {
			"repetitions": [1, 3, 4, 6, 8, 9, 10],
			"batch_size": 128,
			"sample_weight": true,
			"dim": [1, 10],
			"window_size": 0,
			"window_step": 0,
			"time_warping": 0.2,
			"mag_warping": 0.2,
			"noise_snr_db": 25,
			"size_factor": 10,
			"shuffle": true,
			"pad_value": -10.0
		},
		"valid_generator": {
			"repetitions": [2, 5, 7],
			"batch_size": 1,
			"sample_weight": false,
			"dim": [1, 10],
			"window_size": 0,
			"window_step": 0,
			"shuffle": false,
			"update_after_epoch": false,
			"pad_value": -10.0
		}
	},

	"model": {
		"name": "TCCNet",
		"save": true,
		"save_file": "TCCNet_att",
		"extra": {
			"n_dropout": 0.05,
			"n_l2": 0.0005,
			"residual_blocks": 4,
			"tcn_layers": [2,2,2,1],
			"filters": [[32,32], [32,32], [64,64], [128]],
			"classify_as": "att",
			"masking": true
		}
	},

	"training": {
		"optimizer": "adam",
		"epochs": 30,
		"l_rate": 0.01,
		"l_rate_schedule": {
			"lr_start": 0.01,
			"schedule_type": "constant"
		}
	},

	"logging": {
		"enable": true,
		"log_file": "TCCNet_att"
	}

}



